     "text": [
      "enter radius:9\n",
      "enter angle:60\n",
      "area of a segment of circle is: 42.411500823462205\n"
     ]
    }
   ],
   "source": [
    "import math as m\n",
    "r=int(input(\"enter radius:\"))\n",
    "a=int(input(\"enter angle:\"))\n",
    "print(\"area of a segment of circle is:\",(m.pi*r*r)*(a/360))"
